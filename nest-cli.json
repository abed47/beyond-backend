{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "projects": {
    "core-catalog": {
      "type": "library",
      "root": "libs/core-catalog",
      "entryFile": "index",
      "sourceRoot": "libs/core-catalog/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core-catalog/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  },
  "compilerOptions": {
    "webpack": true
  }
}